@model SIAAPI.Models.Comercial.BancoCliente

<div>
    <h3>Detalles de Banco de Cliente: @Html.DisplayFor(model => model.Clientes.Nombre)</h3>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.IDCliente)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Clientes.Nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.IDBanco)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.c_Banco.Nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.CuentaBanco)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CuentaBanco)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.IDMoneda)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.c_Moneda.Descripcion)
        </dd>
       

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "EditB", new { id = Model.IDBancoCliente }, new { @class = "btn btn-success btn-xs" }) |
  @Html.ActionLink("Regresar a la lista", "Details", new { id = Session["IDCliente"] }, new { @class = "btn btn-default btn-xs" })
</p>