
@{
    Layout = null;
}


<html lang="zxx">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Estilos/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/boxicons.min.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/meanmenu.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/animate.min.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Estilos/assets/fonts/flaticon.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/modal-video.min.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/odometer.min.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/style.css">
    <link rel="stylesheet" href="~/Estilos/assets/css/responsive.css">
    <title>Corel - Corporate Business HTML Template</title>
    <link rel="icon" type="image/png" href="~/Estilos/assets/img/favicon.png">
</head>
<body>

    <!-- Preloader -->
    <div class="loader">
        <div class="d-table">
            <div class="d-table-cell">
                <div class="pre-load">
                    <div class="inner one"></div>
                    <div class="inner two"></div>
                    <div class="inner three"></div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Preloader -->
    <!-- Error Section -->
    <div class="error-area">
        <div class="d-table">
            <div class="d-table-cell">
                <div class="error-item">
                    <h1>!!!</h1>
                    <h2>Lo sentimos!! Tu Usuario y Contraseña parecen incorrectas</h2>
                    <p>Por favor verifica tu usuario y contraseña, en caso de persistir favor de comentarnos </p>
                    <a class="common-btn" href="~/MyAccount/Login">Intentar de Nuevo<span></span></a>
                </div>
            </div>
        </div>
    </div>
    <!-- End Error Section -->
    <!-- Go To Top -->
    <div class="go-top">
        <i class="bx bxs-up-arrow-alt"></i>
        <i class="bx bxs-up-arrow-alt"></i>
    </div>
    <!-- End Go To Top -->

    <script src="~/Estilos/assets/js/jquery.min.js"></script>
    <script src="~/Estilos/assets/js/popper.min.js"></script>
    <script src="~/Estilos/assets/js/bootstrap.min.js"></script>
    <script src="~/Estilos/assets/js/form-validator.min.js"></script>
    <script src="~/Estilos/assets/js/jquery.meanmenu.js"></script>
    <script src="~/Estilos/assets/js/wow.min.js"></script>
    <script src="~/Estilos/assets/js/owl.carousel.min.js"></script>
    <script src="~/Estilos/assets/js/jquery-modal-video.min.js"></script>
    <script src="~/Estilos/assets/js/odometer.min.js"></script>
    <script src="~/Estilos/assets/js/jquery.appear.min.js"></script>
    <script src="~/Estilos/assets/js/smoothscroll.min.js"></script>
    <script src="~/Estilos/assets/js/custom.js"></script>
</body>
</html>